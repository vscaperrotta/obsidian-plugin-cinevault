{
	"name": "obsidian-plugin-cinevault",
	"version": "1.0.0",
	"description": "An Obsidian plugin to manage your movie collection and watchlist directly in your vault.",
	"author": "Vittorio Scaperrotta",
	"main": "main.js",
	"engines": {
		"node": "22.16.0"
	},
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"obsidian-plugin",
		"plugin",
		"movie",
		"cinema",
		"vault"
	],
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}